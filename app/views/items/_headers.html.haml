.header-main
  .header-wrapper
    //header1
    .header-first
      .first-group
        =link_to root_path,{class:"mercari"} do
          %img.mercari__logo{alt: "Mercari", src: "https://web-jp-assets.mercdn.net/_next/static/images/logo-acdd90ac4f472d5a6f7a330d33ab1225.svg"}/

      %form.form-box{action: "/jp/search/"}
        %input.form-box__form{name: "keyword", placeholder: "何かお探しですか？", type: "search"}/
        %button.form-box__btn
          %svg.form-box__btn__search{"aria-hidden": "true", height: "16",viewbox: "0 n0 16 16",width: "16"}
            %path{d: "M11.435 10.063h-.723l-.256-.247a5.92 5.92 0 0 0 1.437-3.87 5.946 5.946 0 1 0-5.947 5.947 5.92 5.92 0 0 0 3.87-1.437l.247.256v.723L14.637 16 16 14.637l-4.565-4.574zm-5.489 0A4.111 4.111 0 0 1 1.83 5.946 4.111 4.111 0 0 1 5.946 1.83a4.111 4.111 0 0 1 4.117 4.116 4.111 4.111 0 0 1-4.117 4.117z",fill: "currentColor", "fill-rule": "nonzero"}
    

    - if user_signed_in?
      .header-second
        .second-group
          .category-menu
            %a.header-category{href: "/jp/category/"}
              %svg.category-search{"aria-hidden": "true",height: "16",viewbox: "0 0 20 16",width: "20"}>
                %path{d: "M18.799 15.25h-11.5a1.25 1.25 0 1 1 0-2.5h11.5a1.25 1.25 0 0 1 0 2.5zm0-6h-11.5a1.25 1.25 0 1 1 0-2.5h11.5a1.25 1.25 0 0 1 0 2.5zm0-6h-11.5a1.25 1.25 0 1 1 0-2.5h11.5a1.25 1.25 0 0 1 0 2.5zM2 16a2 2 0 1 0 0-4 2 2 0 0 0 0 4zm0-6a2 2 0 1 0 0-4 2 2 0 0 0 0 4zm0-6a2 2 0 1 0 0-4 2 2 0 0 0 0 4z", fill: "currentColor", "fill-rule": "nonzero"}
              カテゴリーから探す

            .category-menus
              %ul.category-list
                %li.ladies
                  .ladies__lady レディース
                    

                %li.mens
                  .mens__men メンズ
                  %ul.mens-list

                %li.baybes
                  %a.mens__men ベビーキッズ
                  %ul.sc-kcbnda.bOAnU 

                %li.houses
                  %a.hoiuses__house インテリア・住まい・小物
                  %ul.sc-kcbnda.bOAnUl

                %li.books
                  %a.books__book 本・音楽・ゲーム
                  %ul.sc-kcbnda.bOAnUl

                %li.goods
                  %a.goods__good おもちゃ・ボビー・グッズ
                  %ul.sc-kcbnda.bOAnUl

                %li.perfumes
                  %a.perfumes__perfume コスメ・香水・美容
                  %ul.sc-kcbnda.bOAnUl

                %li.elects
                  %a.elects__elect 家電・スマホ・カメラ
                  %ul.sc-kcbnda.bOAnUl

                %li.sports
                  %a.sports__sport スポーツ・レジャー
                  %ul.sc-kcbnda.bOAnUl

                %li.hands
                  %a.hands__hand ハンドメイド
                  %ul.sc-kcbnda.bOAnUl

                %li.tickets
                  %a.tickets__ticket チケット
                  %ul.sc-kcbnda.bOAnUl

                %li.automashine
                  %a.automahsine__auto 自転車・オートバイ
                  %ul.sc-kcbnda.bOAnUl

                %li.other
                  %a.other__another その他
                  %ul.sc-kcbnda.bOAnUl
                  
                %li.category-mode
                  = link_to "カテゴリ一覧", categories_path, class: ".ladies" 

          .brands-menu
            %a.header-brands{:href => "/jp/brand/"}
              %svg.brands-search{"aria-hidden" => "true", :height => "16", :viewbox => "0 0 23 16", :width => "23"}>
                %path{:d => "M6.315 0L4.473 1.806 2.833.019.73 1.977l1.69 1.84L1.198 5.02c-1.594 1.594-1.594 4.188-.01 5.772L6.3 16h13.272a3.08 3.08 0 0 0 3.077-3.077V3.076A3.08 3.08 0 0 0 19.57 0H6.315zm13.46 12.925a.211.211 0 0 1-.205.205H7.504L3.227 8.771a1.226 1.226 0 0 1-.01-1.71L4.38 5.92l1.28 1.371a2.68 2.68 0 1 0 2.13-1.93L6.433 3.907 7.49 2.87h12.082a.211.211 0 0 1 .206.205v9.848h-.002z", :fill => "currentColor", "fill-rule" => "nonzero"}
              ブランドから探す

            .brand-menu
              %ul.brand-list
                %li.shanels
                  .shanels__shanel シャネル
                %li.naikis
                  .naikis__naiki ナイキ
                %li.vitons
                  .vitons__viton ルイ ヴィトン
                %li.suprimes
                  .suprimes__suprime シュプリーム
                %li.adidas
                  .adidas__adida アディダス
                %li.brands
                  .brands__shanel ブランド一覧

        %ul.fours-group
          %li.like-list
